B. Memo and Momo
#include<iostream>
using namespace std;
int main(){
    long long a,b,k;
    cin>>a>>b>>k;
    if(a % k == 0 && b % k == 0){
        cout<<"Both\n";
    }else if(a % k == 0 && b % k != 0){
        cout<<"Memo\n";
    }else if(b % k == 0 && a % k != 0){
        cout<<"Momo\n";
    }else if(a % k != 0 && b % k != 0){
        cout<<"No One\n";
    }
    return 0;
}

//D. Ali Baba and Puzzles
#include<iostream>
using namespace std;
int main(){
    long long a,b,c,d;
    cin>>a>>b>>c>>d;
    if(a + b - c == d){
        cout<<"YES\n";
    }else if(a + b * c == d){
        cout<<"YES\n";
    }else if(a - b + c == d){
        cout<<"YES\n";
    }else if(a - b * c == d){
        cout<<"YES\n";
    }else if(a * b - c == d){
        cout<<"YES\n";
    }else if(a * b + c == d){
        cout<<"YES\n";
    }else{
        cout<<"NO\n";
    }
    return 0;
}
